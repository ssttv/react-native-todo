{
  "parser": "babel-eslint",
  "extends": ["airbnb", "prettier", "prettier/react"],
  "plugins": ["prettier", "react", "react-native"],
  "env": {
    "browser": true
  },
  "ecmaFeatures": {
    "jsx": true
  },
  "rules": {
    "prettier/prettier": "error",
    "react/prop-types": 0,
    "react/jsx-filename-extension": [
      0,
      {
        "extensions": [".js"]
      }
    ]
  }
}
